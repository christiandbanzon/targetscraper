# Target Scraper Environment Variables
# Copy this file to .env and fill in your actual values

# Oxylabs API Credentials
OXYLABS_USERNAME=your_oxylabs_username_here
OXYLABS_PASSWORD=your_oxylabs_password_here

# API Configuration
API_TIMEOUT=120
API_MAX_RETRIES=3

# Logging Configuration
LOG_LEVEL=INFO

# Optional: Custom Output Directory
OUTPUT_DIR=outputs

# Optional: Custom API Base URL (if using different Oxylabs endpoint)
# API_BASE_URL=https://realtime.oxylabs.io/v1/queries
